 
<ul class="navbar-nav"style="float:right !important" >
@if ( User.Identity.IsAuthenticated)
{
    <li class="nav-item">
            <a class="nav-link text-dark" asp-area="" asp-controller="" asp-action="" title="Manage">Hello @User.Identity.Name!</a>
    </li>
    <li class="nav-item">
            <form class="form-inline" method="post" asp-controller="Account" asp-action="LogOut">
            <button  type="submit" class="nav-link btn btn-link text-dark">Logout </button>
        </form>
    </li>
}
else
{
        <li class="nav-item">
            <a class="nav-link text-dark" asp-area="" asp-controller="Account" asp-action="SignUp">Register</a>
        </li>
        <li class="nav-item">
            <a class="nav-link text-dark" asp-area="" asp-controller="Account" asp-action="Login">Login</a>
        </li>
}
</ul>
